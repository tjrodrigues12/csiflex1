<Application x:Class="CSIFLEX.PartAnalyzer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CSIFLEX.PartAnalyzer"
             xmlns:pack="clr-namespace:XamlGeneratedNamespace;assembly=MahApps.Metro"
             xmlns:converters="clr-namespace:CSIFLEX.PartAnalyzer.Views.Converters"
             xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro" 
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Controls.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Fonts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Colors.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/Blue.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/baselight.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconMaterial.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconMaterialLight.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconFontAwesome.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconOcticons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconModern.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconEntypo.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MahApps.Metro.IconPacks;component/Themes/PackIconSimpleIcons.xaml" />

            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="DatagridStyle" 
               BasedOn="{StaticResource MetroDataGridColumnHeader}" 
               TargetType="{x:Type DataGridColumnHeader}">
                <Setter Property="Controls:ControlsHelper.ContentCharacterCasing" Value="Normal"/>
                <Setter Property="FontWeight" Value="Light"/> 
                <Setter Property="FontSize" Value="11"/>
            </Style>
            <converters:VisiblityToInverseBooleanConverter x:Key="VisiblityToInverseBooleanConverter"/>
            <BooleanToVisibilityConverter x:Key="BoolToVisibiltyConverter"/>
            <converters:AggregateCycleTimeConverter CycleType="On" x:Key="CycleOnConverter"/>
            <converters:AggregateCycleTimeConverter CycleType="Off" x:Key="CycleOffConverter"/>
            <converters:AggregateCycleTimeConverter CycleType="Setup" x:Key="CycleSetupConverter"/>
            <converters:AggregateCycleTimeConverter CycleType="Other" x:Key="CycleOtherConverter"/>
            <converters:AggregateCycleTimeConverter CycleType="Total" x:Key="CycleTotalConverter"/>
            <converters:StartOrEndTimePartConverter TimeType="Start" x:Key="ItemsStartTimeConverter"/>
            <converters:StartOrEndTimePartConverter TimeType="End" x:Key="ItemsEndTimeConverter"/>
            <ContextMenu x:Key="ColumnChooserMenu"
                         x:Shared="False"
                         DataContext="{Binding Path=PlacementTarget, RelativeSource={RelativeSource Self}}"
                         ItemsSource="{Binding Columns, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}">
                <ContextMenu.ItemContainerStyle>
                    <Style TargetType="MenuItem">
                        <Setter Property="Header"
                                Value="{Binding Header}"/>
                        <Setter Property="AutomationProperties.Name" 
                                Value="{Binding Header}"/>
                        <Setter Property="IsCheckable" 
                                Value="True" />
                        <Setter Property="IsChecked"
                                Value="{Binding Visibility, Mode=TwoWay, Converter={StaticResource VisiblityToInverseBooleanConverter}}" />
                    </Style>
                </ContextMenu.ItemContainerStyle>
            </ContextMenu>
            <Style x:Key="ColumnHeaderStyle" 
                   TargetType="{x:Type DataGridColumnHeader}">
                <Setter Property="ContextMenu" Value="{StaticResource ColumnChooserMenu}" />
                <Setter Property="Controls:ControlsHelper.ContentCharacterCasing" Value="Normal"/>
                <Setter Property="FontWeight" Value="Light"/>
                <Setter Property="FontSize" Value="11"/>
            </Style>
            <Style x:Key="SmallerMetroToggleSwitchButton" TargetType="{x:Type Controls:ToggleSwitchButton}" BasedOn="{StaticResource MetroToggleSwitchButton}">
                <Setter Property="Width" Value="40" />
                <Setter Property="Height" Value="20" />
                <Setter Property="ThumbIndicatorWidth" Value="4" />
            </Style>
            <ContextMenu x:Key="GridItemsContextMenu" >
                <MenuItem Header="Launch Do Some other action"/>
            </ContextMenu>
            <DataTemplate x:Key="HeaderGroupStyle">
                <Border BorderBrush="DimGray" 
                        BorderThickness="0"
                        Width="Auto"
                        CornerRadius="4"
                        Grid.Column="1"
                        Margin="18,0,8,0"
                        Padding="0,0,0,8">
                    <Grid HorizontalAlignment="Center"
                          Background="AliceBlue">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="{Binding Name.WorkId}"
                                   FontWeight="Heavy"
                                   HorizontalAlignment="Center"
                                   Grid.ColumnSpan="3"
                                   FontSize="14"/>
                        <Grid Margin="4,2"
                              ToolTip="This column represents all the collected part time data received from CSIFLEX"
                              Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="132"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Cycle On" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="0"
                                       Grid.Column="1" 
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items, Converter={StaticResource CycleOnConverter}}"
                                       Margin="4,4,0,4" 
                                       HorizontalAlignment="Left"/>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Cycle Off" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="1" 
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items, Converter={StaticResource CycleOffConverter}}"
                                       Margin="4,4,0,4" 
                                       HorizontalAlignment="Left"/>
                            <TextBlock Grid.Row="2"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Cycle Other" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="2"
                                       Grid.Column="1" 
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items, Converter={StaticResource CycleOtherConverter}}"
                                       Margin="4,4,0,4" 
                                       HorizontalAlignment="Left"/>
                            <TextBlock Grid.Row="3"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Cycle Setup" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="3"
                                       Grid.Column="1" 
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items, Converter={StaticResource CycleSetupConverter}}"
                                       Margin="4,4,0,4" 
                                       HorizontalAlignment="Left"/>
                            <TextBlock Grid.Row="4"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Total" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="4"
                                       Grid.Column="1" 
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items, Converter={StaticResource CycleTotalConverter}}"
                                       Margin="4,4,0,4" 
                                       HorizontalAlignment="Left"/>

                        </Grid>
                        <Grid Margin="4,2"
                              Grid.Row="1"
                              Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="132"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0"
                                               Grid.Column="0"
                                               FontWeight="Bold"
                                               Text="Production start time"
                                               ToolTip="Reported production start time"
                                               Margin="4"/>
                            <TextBlock Grid.Row="0"
                                       Grid.Column="1" 
                                       TextAlignment="Right"
                                       Text="{Binding Name.Entity.ProductionStartDate}"
                                       Margin="4,4,0,4" 
                                       HorizontalAlignment="Left"/>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Production end time"
                                       ToolTip="Reported production end time"
                                       Margin="4"/>
                            <TextBlock Grid.Row="1"
                                       Grid.Column="1"
                                       TextAlignment="Right"
                                       Text="{Binding Name.Entity.ProductionEndDate}"
                                       Margin="4"/>
                            <TextBlock Grid.Row="2"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Machine start date" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="2"
                                       Grid.Column="1"
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items,Converter={StaticResource ItemsStartTimeConverter}}"
                                       Margin="4"/>
                            <TextBlock Grid.Row="3"
                                       Grid.Column="0"
                                       FontWeight="Bold"
                                       Text="Machine start date" 
                                       Margin="4"/>
                            <TextBlock Grid.Row="3"
                                       Grid.Column="1"
                                       TextAlignment="Right"
                                       Text="{Binding Path=Items,Converter={StaticResource ItemsEndTimeConverter}}"
                                       Margin="4"/>

                        </Grid> 
                        <Grid Margin="4,2"
                              Grid.Column="2"
                              ToolTip="This column represents the part production received from the ERP system"
                              Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="132"/>
                                <ColumnDefinition Width="75"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0"
                                               Grid.Column="0"
                                               FontWeight="Bold"
                                               Text="Parts made"
                                               ToolTip="Number of reported parts"
                                               Margin="4"/>
                            <TextBlock Grid.Row="0"
                               Grid.Column="1" 
                               TextAlignment="Right"
                               Text="{Binding Name.PartsMade}"
                               Margin="4,4,0,4" HorizontalAlignment="Left" Width="67"/>
                            <TextBlock Grid.Row="1"
                               Grid.Column="0"
                               FontWeight="Bold"
                               Text="Scrapped parts"
                               ToolTip="Reported parts as scrap"
                               Margin="4"/>
                            <TextBlock Grid.Row="1"
                               Grid.Column="1" 
                               TextAlignment="Right"
                               Text="{Binding Name.ScrappedParts}"
                               Margin="4"/>
                            <TextBlock Grid.Row="2"
                               Grid.Column="0"
                               FontWeight="Bold"
                               Text="Scrapped part %"
                               TextWrapping="Wrap" 
                               Margin="4"/>
                            <TextBlock Grid.Row="2"
                               Grid.Column="1" 
                               TextAlignment="Right"
                               Text="{Binding Name.ScrappedPartsPercentage}"
                               VerticalAlignment="Bottom"
                               Margin="4"/>
                        </Grid>
                    </Grid>
                </Border>
            </DataTemplate>

            <DataTemplate x:Key="EntityTemplate">  
                <Grid Margin="4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Border BorderBrush="LightGray" 
                                BorderThickness="1"
                                CornerRadius="4"
                                Margin="4,2">
                            <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                                <TextBlock Text="{Binding MachineName}" 
                                           FontWeight="Heavy"
                                           HorizontalAlignment="Center"
                                           FontSize="14"/>
                                <Grid Margin="4,2"
                                      Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="164"/>
                                        <ColumnDefinition Width="75"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Grid.Row="0"
                                               Grid.Column="0"
                                               FontWeight="Bold"
                                               Text="Reported Production Time"
                                               ToolTip="Reported time in ERP"
                                               Margin="4"/>
                                    <TextBlock Grid.Row="0"
                                               Grid.Column="1" 
                                               TextAlignment="Right"
                                               Text="{Binding ReportedProductionTime}"
                                               Margin="4"/>
                                    <TextBlock Grid.Row="1"
                                               Grid.Column="0"
                                               FontWeight="Bold"
                                               Text="Actual Cycle Time" 
                                               ToolTip="Reported time in machine"
                                               Margin="4"/>
                                    <TextBlock Grid.Row="1"
                                               Grid.Column="1" 
                                               TextAlignment="Right"
                                               Text="{Binding ActualCycleTime}"
                                               Margin="4"/>
                                    <TextBlock Grid.Row="2"
                                               Grid.Column="0"
                                               FontWeight= "Bold"
                                               Text="Cycle time as % of reported quoted time"
                                               TextWrapping="Wrap" 
                                               Margin="4"/>
                                    <TextBlock Grid.Row="2"
                                               Grid.Column="1" 
                                               TextAlignment="Right"
                                               Text="{Binding ActualToReportedPercentage}"
                                               VerticalAlignment="Bottom"
                                               Margin="4"/>
                                </Grid>
                            </Grid>
                        </Border>
                   
                    <Border BorderBrush="LightGray" 
                            BorderThickness="1"
                            CornerRadius="4"
                            Grid.Column="2"
                            Margin="4,2">
                            <StackPanel>
                                <TextBlock Text="{Binding PartNumber}"
                                           HorizontalAlignment="Center"
                                           FontWeight="Heavy"
                                           FontSize="14"/>
                                <StackPanel Orientation="Horizontal">
                                    <Grid Margin="4,2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="85"/>
                                            <ColumnDefinition Width="150"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Setup time"
                                                   ToolTip="Time to setup part"
                                                   Margin="4"/>
                                        <TextBlock Grid.Row="0" 
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding PartSetupTime}"
                                                   Margin="4"   />
                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="&quot;Other&quot; time"
                                                   ToolTip="Time spent in &quot;other&quot; status"
                                                   Margin="4"/>
                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding PartOtherTime}"
                                                   Margin="4"/>
                                        <TextBlock Grid.Row="2"  
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Start time"
                                                   Margin="4"/>
                                        <TextBlock Grid.Row="2"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding ProductionPart.MachinePartPerformance.ProductionStartTime}"
                                                   Margin="4"/>
                                        
                                        <TextBlock Grid.Row="3"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="End time" 
                                                   Margin="4"/>
                                        
                                        <TextBlock Grid.Row="3"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding ProductionPart.MachinePartPerformance.ProductionEndTime}"
                                                   Margin="4"/>
                                </Grid>
                                    <Grid Margin="4,2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="60"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="24"/>
                                            <RowDefinition Height="24"/>
                                            <RowDefinition Height="24"/>
                                        </Grid.RowDefinitions>
                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Cycle on time"
                                                   ToolTip="Time in cycle on"
                                                   Margin="4"/>
                                        
                                        <TextBlock Grid.Row="0"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding PartCycleOnTime}"
                                                   Margin="4 "/>
                                        
                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Cycle off time"
                                                   ToolTip="Time spent in cycle off"
                                                   Margin="4"/>
                                        
                                        <TextBlock Grid.Row="1"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding PartCycleOffTime}"
                                                   Margin="4"/>
                                </Grid>                                
                            </StackPanel>

                            </StackPanel>
                        </Border>
                    <Border BorderBrush="LightGray" 
                                BorderThickness="1"
                                CornerRadius="4"                                
                                Grid.Column="3"
                                Margin="4,2">
                        <StackPanel>
                            <TextBlock HorizontalAlignment="Center"
                                       FontWeight="Heavy"
                                       FontSize="14">
                                <Run Text="Production Task ID"/>
                                <Run  Text="{Binding ProductionPart.ProductionTasksEntity.Id}"/>
                            </TextBlock>
                            <StackPanel Orientation="Horizontal">
                                <Grid Margin="4,2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="120"/>
                                        <ColumnDefinition Width="50"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Grid.Row="0"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Planned Quantity"
                                                   ToolTip="Time to setup part"
                                                   Margin="4"/>
                                    <TextBlock Grid.Row="0" 
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding ProductionPart.ProductionTasksEntity.PlannedQuantity}"
                                                   Margin="4"/>
                                    <TextBlock Grid.Row="1"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Produced Quantity"
                                                   ToolTip="Time spent in &quot;other&quot; status"
                                                   Margin="4"/>
                                    <TextBlock Grid.Row="1"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding ProductionPart.ProductionTasksEntity.ProducedQuantity}"
                                                   Margin="4"/>
                                    <TextBlock Grid.Row="2"
                                                   Grid.Column="0"
                                                   FontWeight="Bold"
                                                   Text="Rejected Quantity"
                                                   ToolTip="Time spent in &quot;other&quot; status"
                                                   Margin="4"/>
                                    <TextBlock Grid.Row="2"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding ProductionPart.ProductionTasksEntity.RejectedQuantity}"
                                                   Margin="4"/>
                                </Grid>
                                <Grid Margin="4,2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="135"/>
                                        <ColumnDefinition Width="150"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock Grid.Row="0"
                                               Grid.Column="0"
                                               FontWeight="Bold"
                                               Text="Production Start Date" 
                                               Margin="4"/>
                                    <TextBlock Grid.Row="0"
                                                   Grid.Column="1" 
                                                   TextAlignment="Right"
                                                   Text="{Binding ProductionPart.ProductionTasksEntity.ProductionStartDate}"
                                                   Margin="4 "/>
                                    <TextBlock Grid.Row="1"
                                               Grid.Column="0"
                                               FontWeight="Bold"
                                               Text="Production End Date" 
                                               Margin="4"/>
                                    <TextBlock Grid.Row="1"
                                               Grid.Column="1" 
                                               TextAlignment="Right"
                                               Text="{Binding ProductionPart.ProductionTasksEntity.ProductionEndDate}"
                               Margin="4"/>
                                </Grid>
                            </StackPanel>

                        </StackPanel>
                    </Border>
                </Grid> 
            </DataTemplate>

            <CollectionViewSource x:Key="GroupedDataList" 
                                  Source="{Binding EntityCollection ,RelativeSource={RelativeSource TemplatedParent}}">
                <CollectionViewSource.GroupDescriptions>
                    <PropertyGroupDescription PropertyName="Title"/>
                </CollectionViewSource.GroupDescriptions>
            </CollectionViewSource>
        </ResourceDictionary>
    </Application.Resources>
</Application>
